    #d-bannar.carousel.slide(data-ride='carousel')
      // Indicators
      ol.carousel-indicators
        li.active(data-target='#d-bannarr', data-slide-to='0')
        li(data-target='#d-bannar', data-slide-to='1')
        li(data-target='#d-bannar', data-slide-to='2')
      // Slides
      .carousel-inner(role='listbox')
        .carousel-item.w-100.active
          .bg-mask
          .bg(style='background:url(../img/banner/cat_d/banner_cat_d_01.png) no-repeat center; background-size: cover')
